<!-- app/views/users/index.html.erb -->
<h1>Users Page</h1>
<!-- ここにコンテンツを記述 -->
<h1>index</h1>

<h1>ユーザー一覧</h1>

<% @users.each do |user| %>
  <p>name</p>
  <%= user.name %>
  <%= link_to "show", user_path(user) %>

<% end %>

<h1>New book</h1>
<%= form_with model: @new_book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Opinion</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
<% end %>